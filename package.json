{
	"name": "easy-covid-data",
	"version": "0.0.1",
	"private": false,
	"description": "A tool to generate easy to use data related to the Covid-19 pandemic",
	"repository": {
		"type": "git",
		"url": "https://github.com/Coriou/easy-covid-data.git"
	},
	"license": "ISC",
	"author": "Benjamin Coriou",
	"main": "build/main.js",
	"scripts": {
		"build": "backpack build",
		"dev": "backpack",
		"format": "prettier --write '**/*.{js,json,md}'",
		"lint": "eslint --ext .js src",
		"prepublishOnly": "sort-package-json && npm run lint && npm run format"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"country-json": "^1.1.0",
		"data-forge": "^1.8.4",
		"topojson": "^3.0.2",
		"world-atlas": "^2.0.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run prepublishOnly"
		}
	},
	"devDependencies": {
		"backpack-core": "^0.8.4",
		"eslint": "^6.8.0",
		"husky": "^4.2.3",
		"prettier": "^2.0.2",
		"sort-package-json": "^1.40.0"
	}
}
